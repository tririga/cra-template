{
  "package": {
    "dependencies": {
      "@carbon/icons-react": "^11.20.0",
      "@carbon/react": "^1.31.3",
      "@carbon/themes": "^11.19.0",
      "@testing-library/jest-dom": "^5.16.5",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^13.5.0",
      "@tririga/tririga-react-components": "^1.4.3-beta",
      "classnames": "^2.3.2",
      "env-cmd": "^10.1.0",
      "prop-types": "^15.8.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.13.0",
      "react-scripts": "5.0.1",
      "sass": "^1.62.1",
      "web-vitals": "^2.1.4"
    },  
   "devDependencies": {
      "@babel/eslint-parser": "^7.21.8",
      "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
      "@babel/preset-react": "^7.22.5",
      "eslint": "^8.43.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-config-standard": "^17.1.0",
      "eslint-config-standard-react": "^13.0.0",
      "eslint-plugin-jest": "^27.2.2",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^6.1.1",
      "eslint-plugin-react": "^7.32.2",
      "husky": "^8.0.3",
      "prettier": "^2.8.8",
      "pretty-quick": "^3.1.3"
  },
    "homepage": ".",
    "scripts": {
      "start": "SASS_PATH=\"`cd \"./node_modules/@carbon\";pwd`\" react-scripts start",
      "build": "SASS_PATH=\"`cd \"./node_modules/@carbon\";pwd`\" react-scripts build",
      "deploy": "env-cmd -x -f ./.env.development.local tri-deploy -t \\$REACT_APP_TRIRIGA_URL -u \\$TRIRIGA_USER -p \\$TRIRIGA_PASSWORD -c -m 3",
      "build:deploy": "npm run build && npm run deploy",
      "lint": "./node_modules/.bin/eslint src",
      "lint-fix": "./node_modules/.bin/eslint --fix src --ext .js",
      "pretty-quick": "pretty-quick"
    },
    "husky": {
      "hooks": {
        "pre-commit": "npm run pretty-quick && npm run lint",
        "pre-push": "npm run lint"
      }
    },
    "eslintConfig": {}
  }
}
